{
  "name": "functions",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "prebuild": "tsx scripts/generate-version.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:analyzer": "vitest run ai-request-analyzer",
    "test:analyzer:watch": "vitest ai-request-analyzer",
    "test:video": "vitest run ai-request-analyzer -- -t \"video:\"",
    "test:image": "vitest run ai-request-analyzer -- -t \"image:\"",
    "test:audio": "vitest run ai-request-analyzer -- -t \"audio:\"",
    "test:text": "vitest run ai-request-analyzer -- -t \"text:\"",
    "debug:text": "npm run build && node lib/ai-request-analyzer/debug-reasoning.js"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google/genai": "^1.27.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.6.0",
    "mime-types": "^3.0.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "private": true
}
